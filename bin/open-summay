#!/bin/bash

#-----------------------------------------------------------------------------
# Prelude
#-----------------------------------------------------------------------------

source $(dirname $0)/../conf/piglit-run.conf

function no() {
    echo "no!"
    exit 1
}

#-----------------------------------------------------------------------------
# Parse args
#-----------------------------------------------------------------------------

if [ $# -ne 0 ]; then
    no
fi

#-----------------------------------------------------------------------------
# Run
#-----------------------------------------------------------------------------

gnome-open $piglit_run_dir/summary/index.html

# vim: tw=0:
