#!/bin/bash

#-----------------------------------------------------------------------------
# Prelude
#-----------------------------------------------------------------------------

source $(dirname $0)/../conf/piglit-run.conf

function no() {
    echo "no!"
    exit 1
}


#-----------------------------------------------------------------------------
# Run
#-----------------------------------------------------------------------------

if [ $# -ne 0 ]; then no; fi
if [ -d $piglit_run_dir/results ]; then rm -r $piglit_run_dir/results; fi

# vim: tw=0:

